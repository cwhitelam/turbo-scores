{
  "name": "turbo-scores",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "analyze": "npm run build:analyze",
    "build:prod": "tsc && vite build --mode production",
    "lint": "eslint . --max-warnings 100",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e:dev": "cypress open",
    "lint:fix": "eslint . --fix --max-warnings 100",
    "lint:lenient": "eslint . --rule '@typescript-eslint/no-explicit-any: off' --rule '@typescript-eslint/no-unused-vars: warn' --max-warnings 20",
    "format": "prettier --write .",
    "validate": "npm-run-all --parallel type-check lint test build",
    "type-check": "tsc --noEmit",
    "find-large-components": "node scripts/findLargeComponents.js",
    "analyze:imports": "node scripts/optimizeImports.js",
    "fix:imports": "node scripts/optimizeImports.js --fix",
    "analyze:packages": "node scripts/analyzePackages.js",
    "optimize:svgs": "node scripts/optimizeSvgs.js",
    "optimize": "node scripts/optimizeBundleSize.js",
    "optimize:fix": "node scripts/optimizeBundleSize.js --fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@lucide/lab": "^0.1.2",
    "@tanstack/react-query": "^5.62.16",
    "compression": "^1.7.4",
    "date-fns-tz": "^3.2.0",
    "express": "^4.18.2",
    "lucide-react": "^0.309.0",
    "node-fetch": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cypress": "^13.6.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.37.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.29.1",
    "vite": "^5.0.8",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}